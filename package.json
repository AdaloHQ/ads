{
  "name": "ads",
  "version": "1.0.1",
  "author": "Adalo",
  "license": "MIT",
  "main": "index.js",
  "description": "Display Google AdMob banner with customized size.",
  "scripts": {
    "start": "adalo dev",
    "build": "adalo build",
    "login": "adalo login",
    "publish": "adalo publish",
    "start:local": "ADALO_DEV_SERVER=http://localhost:3035 yarn start",
    "login:local": "ADALO_BACKEND_SERVER=http://localhost:8084 yarn run login",
    "publish:local": "REGISTRY_URL=http://localhost:8000 yarn run publish",
    "deploy": "adalo deploy",
    "deploy:local": "REGISTRY_URL=http://localhost:8000 yarn run deploy"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16",
    "react-native": "*"
  },
  "adalo": {
    "displayName": "AdMob Banner",
    "components": [
      {
        "name": "Ads",
        "manifest": "./src/components/Ads/manifest.json"
      }
    ],
    "iosInstallScript": "scripts/install_ios.sh",
    "androidInstallScript": "scripts/install_android.sh"
  },
  "devDependencies": {
    "@adalo/cli": "^0.0.37",
    "react": "^16.5.2",
    "react-art": "^16.6.0",
    "react-dom": "^16.5.2",
    "react-native-web": "^0.9.5"
  },
  "dependencies": {
    "json-easy-filter": "^0.3.1",
    "react-native-admob": "^2.0.0-beta.6"
  }
}
