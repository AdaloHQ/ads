{
  "displayName": "Ads",
  "defaultWidth": 320,
  "defaultHeight": 50,
  "components": "./index.js",
  "resizeX": false,
  "resizeY": false,
  "props": [
    {
      "name": "size",
      "displayName": "Size",
      "type": "text",
      "default": "banner",
      "control": {
        "type": "menu",
        "options": [
          { "label": "Standard", "value": "banner" },
          { "label": "Large", "value": "largeBanner" },
          { "label": "Medium Rectangle", "value": "mediumRectangle" }
        ]
      }
    },
    {
      "name": "iosAppID",
      "displayName": "iOS App ID",
      "type": "text",
      "placeholder": "ca-app-pub-XXXXXXXXXXXXXXXX~XXXXXXXXXX"
    },
    {
      "name": "iosAdID",
      "displayName": "iOS Ad ID",
      "type": "text",
      "placeholder": "ca-app-pub-XXXXXXXXXXXXXXXX/XXXXXXXXXX"
    },
    {
      "name": "andAppID",
      "displayName": "Android App ID",
      "type": "text",
      "placeholder": "ca-app-pub-XXXXXXXXXXXXXXXX~XXXXXXXXXX"
    },
    {
      "name": "andAdID",
      "displayName": "Android Ad ID",
      "type": "text",
      "placeholder": "ca-app-pub-XXXXXXXXXXXXXXXX/XXXXXXXXXX"
    }
  ]
}